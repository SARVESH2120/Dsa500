    public static void EvenNo(int n) {
        if (n % 2 == 0) {
            System.out.println("Even");
        } else {
            System.out.println("Odd");
        }
    }

    public static void ComparisonOfTwoNumber(int a, int b) {
        if (a > b) {
            System.out.println("a is greater" + a);
        } else {
            System.out.println("B is greater" + b);
        }
    }

    public static void GradeSystem(int marks) {
        if (marks > 90) {
            System.out.println("Grade A ");
        } else if (marks > 80) {
            System.out.println("Grade B");
        } else if (marks < 80) {
            System.out.println("Grade C");
        }
    }

    public static void TakingInputIntAndStringTogether() {
        Scanner sc = new Scanner(System.in);
        int t = Integer.parseInt(sc.nextLine());
        String YourName = sc.nextLine();
        System.out.println("The no. is " + t + "and your name is " + YourName);

    }

    public static void PrimeNumber() {
        Scanner sc = new Scanner(System.in);
        System.out.println("How many no. want to check it is prime enter ");
        int t = sc.nextInt();
        for (int i = 0; i < t; i++) {
            System.out.println("Enter  a number");
            int n = sc.nextInt();
            int count = 0;
            for (int div = 1; div <= n; div++) {
                if (n % div == 0) {
                    count++;
                }
            }
            if (count == 2) {
                System.out.println("Prime");
            } else {
                System.out.println("Not Prime");
            }
        }
    }

    public static void OptimizeApproachPrimeNumber() {
        Scanner sc = new Scanner(System.in);
        System.out.println("How many no. want to check it is prime enter ");
        int t = sc.nextInt();
        for (int i = 0; i < t; i++) {
            System.out.println("Enter a number ");
            int n = sc.nextInt();
            int count = 0;
            for (int div = 2; div * div <= n; div++) {
                if (n % div == 0) {
                    count++;
                    break;
                }
            }
            if (count == 0) {
                System.out.println("Prime");
            } else {
                System.out.println("not prime");
            }
        }
    }

    public static void PrintAllPrimeBetweenTheRange() {
        Scanner sc = new Scanner(System.in);
        int low = sc.nextInt();
        int high = sc.nextInt();
        for (int i = low; i <= high; i++) {
            int count = 0;
            for (int div = 2; div * div <= i; div++) {
                if (i % div == 0) {
                    count++;
                    break;
                }
            }
            if (count == 0) {
                System.out.println(i);
            }
        }
    }

    public static void FibonacciNumber() {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int a = 0;
        int b = 1;

        for (int i = 0; i <= n; i++) {
            System.out.println(a);
            int c = a + b;
            a = b;
            b = c;
        }
    }

    public static void CountDigit() {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int cnt = 0;
        while (n != 0) {
            n = n / 10;
            cnt++;
        }
        System.out.println(cnt);
    }

    public static void DigitsofANumber() {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int numberOfDigit = 0;
        int temp = n;

        while (temp != 0) {
            temp = temp / 10;
            numberOfDigit++;
        }
        int div = (int) Math.pow(10, numberOfDigit - 1);
        while (div != 0) {
            int q = n / div;
            System.out.println(q);
            n = n % div;
            div = div / 10;
        }
    }

